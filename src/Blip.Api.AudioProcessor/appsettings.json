{
  "AzureSpeechSubscriptionKey": "YOUR-API-KEY",
  "AzureSpeechRecognitionLanguage": "pt-BR",
  "AzureRegion": "westus",
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Settings": {
    "BlipBotSettings": {
      "Identifier": "botBlipIdentifier",
      "AccessKey": "botBlipAccessKey",
      "Authorization": "botAuthorizationKey"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
